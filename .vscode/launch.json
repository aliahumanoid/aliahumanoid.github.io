{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Open with Live Server",
      "type": "chrome",
      "request": "launch",
      "file": "${workspaceFolder}/index.html",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Open with Python HTTP Server",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:8000",
      "webRoot": "${workspaceFolder}",
      "preLaunchTask": "Start Python HTTP Server"
    }
  ]
}

